{
  "presets": ["react", "es2015", "stage-0"],
  "env": {
    "development": {
      "presets": ["react-hmre"],
      "plugins": [
        "add-module-exports",
        "transform-class-properties",
        "react-html-attrs", 
        "transform-decorators-legacy", 
        "transform-object-rest-spread",
        "transform-object-assign"        
      ]
    },
    "production": {
      "plugins": [
        "add-module-exports",
        "transform-class-properties",
        "react-html-attrs", 
        "transform-decorators-legacy", 
        "transform-object-rest-spread",
        "transform-object-assign"
      ]
    }
  }
}